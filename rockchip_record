[rk356x] [patch] Android 13 boot from sdcard failed
device/rockchip/rk356x/BoardConfig.mk

-PRODUCT_BOOT_DEVICE := fe310000.sdhci,fe330000.nandc
+PRODUCT_BOOT_DEVICE := fe310000.sdhci,fe330000.nandc,fe2b0000.dwmmc
